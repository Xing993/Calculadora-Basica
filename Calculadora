print("CALCULADORA\n")

        
num1, num2 = int(input("Ingrese un numero ")), int(input("Ingrese otro numero "))
suma = num1 + num2
resta = num1 - num2
multi = num1 * num2
resul = num1 / num2
pow = num1 ** num2

print(f"La suma es: {suma}")
print(f"La resta es: {resta}")
print(f"La multiplicacion es: {multi}")
print(f"La division es: {resul}")
print(f"La potencia es: {pow}")
